package indra.gesvoto.services.datos;

import java.io.Serializable;
import javax.persistence.*;
import java.util.List;


/**
 * The persistent class for the VOTOS database table.
 * 
 */
@Entity
@Table(name="VOTOS")
@NamedQuery(name="Voto.findAll", query="SELECT v FROM Voto v")
public class Voto implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	@Column(unique=true, nullable=false)
	private int id;

	@Column(length=45)
	private String VOTOScol;

	//bi-directional many-to-many association to Concepto
	@ManyToMany
	@JoinTable(
		name="VOTOS_CONCEPTOS"
		, joinColumns={
			@JoinColumn(name="VOTOS_ID", nullable=false)
			}
		, inverseJoinColumns={
			@JoinColumn(name="CONCEPTOS_ID", nullable=false)
			}
		)
	private List<Concepto> conceptos;

	public Voto() {
	}

	public int getId() {
		return this.id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getVOTOScol() {
		return this.VOTOScol;
	}

	public void setVOTOScol(String VOTOScol) {
		this.VOTOScol = VOTOScol;
	}

	public List<Concepto> getConceptos() {
		return this.conceptos;
	}

	public void setConceptos(List<Concepto> conceptos) {
		this.conceptos = conceptos;
	}

}