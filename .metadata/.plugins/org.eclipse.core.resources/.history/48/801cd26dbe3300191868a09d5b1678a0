package indra.gesvoto.services.datos;

import java.io.Serializable;
import javax.persistence.*;
import java.util.Date;


/**
 * The persistent class for the CONCURSOS database table.
 * 
 */
@Entity
@Table(name="CONCURSOS")
@NamedQuery(name="Concurso.findAll", query="SELECT c FROM Concurso c")
public class Concurso implements Serializable {
	private static final long serialVersionUID = 1L;
	private ConcursoPK id;
	private String descripcion;
	private Date fecha;
	private String titulo;
	private Role role;

	public Concurso() {
	}


	@EmbeddedId
	public ConcursoPK getId() {
		return this.id;
	}

	public void setId(ConcursoPK id) {
		this.id = id;
	}


	@Column(nullable=false, length=45)
	public String getDescripcion() {
		return this.descripcion;
	}

	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}


	@Temporal(TemporalType.TIMESTAMP)
	@Column(nullable=false)
	public Date getFecha() {
		return this.fecha;
	}

	public void setFecha(Date fecha) {
		this.fecha = fecha;
	}


	@Column(nullable=false, length=45)
	public String getTitulo() {
		return this.titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}


	//bi-directional many-to-one association to Role
	@ManyToOne
	@JoinColumn(name="ROLES_ID", nullable=false, insertable=false, updatable=false)
	public Role getRole() {
		return this.role;
	}

	public void setRole(Role role) {
		this.role = role;
	}

}